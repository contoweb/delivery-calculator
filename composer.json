{
    "name": "contoweb/delivery-calculator",
    "description": "Delivery calculator for Laravel to calculate a date time by providing the start date time and a duration in hours",
    "keywords": [
        "laravel",
        "delivery",
        "calculation"
    ],
    "require": {
        "nesbot/carbon": "^2.0",
        "illuminate/database": ">=6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5" 
    },
    "license": "MIT",
    "authors": [
        {
            "name": "contoweb AG",
            "email": "info@contoweb.ch"
        }
    ],
    "autoload": {
        "psr-4": {
            "Contoweb\\DeliveryCalculator\\": "src"
        }
	},
    "autoload-dev": {
        "psr-4": {
            "Contoweb\\DeliveryCalculator\\Test\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Contoweb\\DeliveryCalculator\\DeliveryCalculatorServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
